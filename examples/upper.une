# Turn Text Uppercase

def char_upper(char)
{
  if ord(char) < ord("a") || ord(char) > ord("z")
    return char
  return chr(ord(char)+ord("A")-ord("a"))
}

def upper(string, length)
{
  out = ""
  for i from 0 till length
    out = out + char_upper(string[i])
  return out
}

input = "abcdefghijklmnopqrstuvwxyz012356789"

print(upper(input, len(input)))