# Turn Text Uppercase

char_upper = function(char)
{
  if ord(char) < ord("a") || ord(char) > ord("z")
    return char
  return chr(ord(char)+ord("A")-ord("a"))
}

upper = function(string)
{
  out = ""
  for i from 0 till len(string)
    out = out + char_upper(string[i])
  return out
}

print(upper("abcdefghijklmnopqrstuvwxyz012356789"))
